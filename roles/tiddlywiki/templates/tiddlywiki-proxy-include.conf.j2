        <Proxy *>
            Order Deny,Allow
            Allow from all
        </Proxy>

        RewriteCond %{REQUEST_FILENAME} !-F
        RewriteCond %{REQUEST_URI} ^/[^/]+\.html$
        RewriteCond %{REQUEST_URI} !^/(status$|bags/|recipes/|static/|s/|favicon\.ico$)
        RewriteRule ^/(.+)$ /static/$1

        RewriteCond %{REQUEST_FILENAME} !-F
        RewriteCond %{REQUEST_URI} !^/[^/]+\.html$
        RewriteCond %{REQUEST_URI} ^/[^/]+\.[a-z0-9]{2,4}$
        RewriteCond %{REQUEST_URI} !^/(status$|bags/|recipes/|static/|s/|favicon\.ico$)
        RewriteRule ^/(.+)$ /s/$1

        RewriteCond %{REQUEST_FILENAME} !-F
        RewriteCond %{REQUEST_URI} !^/[^/]+\.html$
        RewriteCond %{REQUEST_URI} !^/(status$|bags/|recipes/|static[/\.]|s/|favicon\.ico$)
        RewriteRule ^/(.+)$ /static/$1.html

        ProxyPreserveHost Off
        ProxyPass /static !
        ProxyPass /s !
        #ProxyPass /static.css !
        #ProxyPass /favicon.ico !
        #ProxyPass /sitemap.xml !
        #ProxyPass /ror.xml !
        ProxyPass / http://127.0.0.1:8080/
        AllowEncodedSlashes on
